<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.xws111.sqlpractice.mapper.TagMapper">

    <resultMap id="BaseResultMap" type="com.xws111.sqlpractice.model.entity.Tag">
            <id property="id" column="tag_id" jdbcType="SMALLINT"/>
            <result property="name" column="tag_name" jdbcType="VARCHAR"/>
            <result property="createTime" column="tag_create_time" jdbcType="TIMESTAMP"/>
    </resultMap>

    <sql id="Base_Column_List">
        tag_id,tag_name,tag_create_time
    </sql>
</mapper>
